/* Move down content because we have a fixed navbar that is 50px tall */
body { 
	padding-bottom:80px;
}

/******************************************
		Header CSS
*******************************************/
#header .navbar { 
	margin-bottom:0px; 
    border-radius: 0px;
}

#header .navbar-inverse { 
	background: -moz-linear-gradient(top, rgba(255, 255, 255, 1.0) 0%, rgba(255, 255, 255, 1.0) 100%); 
	/* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, rgba(255, 255, 255, 1.0)), color-stop(100%, rgba(255, 255, 255, 1.0))); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top, rgba(255, 255, 255, 1.0) 0%, rgba(255, 255, 255, 1.0) 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top, rgba(255, 255, 255, 1.0) 0%, rgba(255, 255, 255, 1.0) 100%); 
	/* Opera11.10+ */
	background: -ms-linear-gradient(top, rgba(255, 255, 255, 1.0) 0%, rgba(255, 255, 255, 1.0) 100%); 
	/* IE10+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#ffffff',GradientType=0 ); /* IE6-9 */
	background: linear-gradient(top, rgba(255, 255, 255, 1.0) 0%, rgba(255, 255, 255, 1.0) 100%); 
	/* W3C */
	border-color: transparent;
}
#header .navbar-inverse .navbar-nav > li > a { 
	color: rgb(47,176,235); 
}

#header .navbar-inverse {
    border: 0px;
}

#header .navbar-inverse .navbar-brand {
    padding: 5px;
}

#header .navbar-inverse .navbar-nav {
	padding: 5px;
}

#header .navbar-inverse .navbar-nav > li > a,
#header .navbar-inverse .navbar-nav > li > a:hover { 
	color: rgb(47,176,235); 
	font-size:1.1em;
	text-shadow:none;
	font-weight:normal;
}

#header .navbar-inverse .navbar-nav > .active > a, 
#header .navbar-inverse .navbar-nav > .active > a:hover, 
#header .navbar-inverse .navbar-nav > .active > a:focus  {
	background: rgb(47,176,235);
	color: rgba(255, 255, 255, 1.0);
}
 	
#header .navbar-inverse .navbar-toggle {
    border-color: transparent;
    background-color: transparent;
}

#header .navbar-inverse .navbar-toggle:hover,
#header .navbar-inverse .navbar-toggle:focus {
    border-color: rgb(47,176,235);
    background-color: transparent;
}

#header .panel-default {
    border-color: rgba(255, 255, 255, 1.0);
}

#header .panel-default > .panel-heading {
    background: -moz-linear-gradient(top, rgba(255, 255, 255, 1.0) 0%, rgba(255, 255, 255, 1.0) 100%); 
	/* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, rgba(255, 255, 255, 1.0)), color-stop(100%, rgba(255, 255, 255, 1.0))); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top, rgba(255, 255, 255, 1.0) 0%, rgba(255, 255, 255, 1.0) 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top, rgba(255, 255, 255, 1.0) 0%, rgba(255, 255, 255, 1.0) 100%); 
	/* Opera11.10+ */
	background: -ms-linear-gradient(top, rgba(255, 255, 255, 1.0) 0%, rgba(255, 255, 255, 1.0) 100%); 
	/* IE10+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#ffffff',GradientType=0 ); /* IE6-9 */
	background: linear-gradient(top, rgba(255, 255, 255, 1.0) 0%, rgba(255, 255, 255, 1.0) 100%); 
	/* W3C */
    color: rgb(47,176,235);
}

#header .dropdown-menu { 
	background: -moz-linear-gradient(top, rgba(255, 255, 255, 1.0) 0%, rgba(255, 255, 255, 1.0) 100%); 
	/* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, rgba(255, 255, 255, 1.0)), color-stop(100%, rgba(255, 255, 255, 1.0))); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top, rgba(255, 255, 255, 1.0) 0%, rgba(255, 255, 255, 1.0) 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top, rgba(255, 255, 255, 1.0) 0%, rgba(255, 255, 255, 1.0) 100%); 
	/* Opera11.10+ */
	background: -ms-linear-gradient(top, rgba(255, 255, 255, 1.0) 0%, rgba(255, 255, 255, 1.0) 100%); 
	/* IE10+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#ffffff',GradientType=0 ); /* IE6-9 */
	background: linear-gradient(top, rgba(255, 255, 255, 1.0) 0%, rgba(255, 255, 255, 1.0) 100%); 
	/* W3C */
} 

#header .dropdown-menu > li > a { 
	color: rgb(47,176,235); 
}

#header .dropdown-menu > li > a:hover, 
#header .dropdown-menu > li > a:focus {
	background: none;
}    

#header .ent-nav-menu {
  position: relative;
  display: inline-block;
  padding: 10px;
  font-weight: 500;
}

