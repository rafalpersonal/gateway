/************************************
		Panel CSS
************************************/

.panel-default .panel-heading {
	font-weight: bold;	
	color: rgb(47,176,235); 
	font-size: 1.4em;
}

/*************************************
	 	Jumbotron CSS
*************************************/
.jumbotron {
	padding:10px;
}

