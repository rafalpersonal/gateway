	background: -webkit-linear-gradient(top, rgba(255, 255, 255, 1.0) 0%, rgba(255, 255, 255, 1.0) 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top, rgba(255, 255, 255, 1.0) 0%, rgba(255, 255, 255, 1.0) 100%); 
